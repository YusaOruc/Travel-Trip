@using TravelTripProje.Models.Siniflar
@model List<Blog>

<div class="box1-left">
    @foreach (var y in Model)
    {
        <div class="bot-img">
            <img style="width:180px;height:75px;" src="@y.BlogImage" alt="" />
        </div>
        <div class="bot-desc1">
            <p><a href="/Blog/BlogDetay/@y.ID">@y.Baslik</a></p> 
            <p>@y.Aciklama.Substring(0, 30) ... </p>
        </div>
        <div class="clear"></div>
    }


</div>
